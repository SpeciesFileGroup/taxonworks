{
  "extends": [
    "standard",
    "eslint:recommended",
    "plugin:vue/vue3-strongly-recommended",
    "prettier"
  ],
  "plugins": ["vue", "import"],
  "env": {
    "browser": true,
    "es2021": true,
    "jquery": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "TW": true,
    "jquery": true,
    "defineEmits": true,
    "defineProps": true,
    "defineExpose": true
  },
  "rules": {
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always"
      }
    ]
  }
}
