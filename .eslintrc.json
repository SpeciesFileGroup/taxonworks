{
  "extends": [
    "standard",
    "eslint:recommended",
    "plugin:vue/vue3-strongly-recommended"
  ],
  "plugins": [
    "vue",
    "import"
  ],
  "env": {
    "browser": true,
    "es2021": true,
    "jquery": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "TW": true,
    "jquery": true,
    "defineEmits": true,
    "defineProps": true,
    "defineExpose": true
  }
}
