<%= content_for :add do -%>
  <%= content_tag(:li, link_to('New asserted distribution', new_asserted_distribution_task_path(otu_id: @otu.id), data: {turbolinks: false} )) %>
<% end %>

<%= content_for :report do -%>
  <%= content_tag(:li, link_to('Browse', browse_otus_task_path(@otu), data: {turbolinks: false} )) %>
  <%= content_tag(:li, link_to('Contents', preview_otu_content_path(@otu.id), 'data-turbolinks' => false)) %>
<% end %>

<%= content_for :associated do -%>
  <%= on_loan_tag(@otu) -%>
<% end %>

<%= content_for :icon_navigation do -%>
  <%= content_tag(:li, browse_otu_link(@otu) )   %>
  <%= content_tag(:li, otus_radial(@otu) )   %>
<% end %>

<%= render(partial: 'shared/data/project/show', locals: {object: @otu}) -%>
