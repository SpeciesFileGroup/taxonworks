<%= render("/shared/data/all/list/list_header", objects: @sequences) -%>

<table class="tablesorter">
  <thead>
  <tr>
    <%= fancy_th_tag(name: 'Name') -%>
    <%= fancy_th_tag(name: 'Sequence') -%>
    <%= fancy_th_tag(name: 'Sequence type') -%>
    <%= fancy_th_tag(name: 'Updated by', group: 'housekeeping') -%>
    <%= fancy_th_tag(name: 'Last updated', group: 'housekeeping') -%>
  </tr>
  </thead>

  <tbody>
  <% @sequences.each do |sequence| %>
      <%= content_tag(:tr, class: :contextMenuCells) do -%>
          <td><%= sequence.name %></td>
          <td><%= sequence.sequence %></td>
          <td><%= sequence.sequence_type %></td>
          <%= fancy_metadata_cells_tag(sequence) -%>
      <% end %>
  <% end %>
  </tbody>
</table>

