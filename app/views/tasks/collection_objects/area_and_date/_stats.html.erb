<% a = count %>
<%= "#{a} total records - created on by" -%>

<% if (a < 1000) %>
    day
    <%= line_chart(objects.group_by_day("created_at").count) %>
<% elsif (a < 10000) %>
    week
    <%= line_chart(objects.group_by_week("created_at").count) %>
<% elsif (a < 100000) %>
    month
    <%= line_chart(objects.group_by_month("created_at").count) %>
<% else %>
    year
    <%= line_chart(objects.group_by_year("created_at").count) %>
<% end %>
