<table border="1">
  <tr>
    <th></th> <%= csv.headers.collect { |h| content_tag(:th, h) }.join.html_safe -%>  </tr>
  <% csv.each_with_index do |row, i| -%>
      <tr>
        <td><%= link_to(i + 1, "#parse_#{i + 1}", id: "line_#{i + 1}").html_safe -%></td>
        <%= row.fields.collect { |c| content_tag(:td, c) }.join.html_safe -%>
      </tr>
  <% end %>
</table>
